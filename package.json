{
  "name": "vite-plugin-cssvg",
  "version": "1.0.8",
  "description": "Vite plugin to transform SVG files into css/less/sass inline image",
  "keywords": [
    "transform SVG",
    "css",
    "css inside",
    "svg inline",
    "svg Alias support in Css"
  ],
  "repository": "https://github.com/MySetsuna/vite-plugin-cssvg",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "type": "module",
  "license": "MIT",
  "scripts": {
    "build": "rimraf dist && tsc -p .&& uglifyjs dist/index.js -o dist/index.js",
    "dev": "tsc -w -p ."
  },
  "files": [
    "dist"
  ],
  "author": "Xuekai Jiang",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "dependencies": {
    "@rollup/pluginutils": "^5.0.5"
  },
  "peerDependencies": {
    "vite": "^2.6.0 || 3 || 4"
  },
  "devDependencies": {
    "@types/node": "^20.8.9",
    "rimraf": "^5.0.5",
    "typescript": "^5.2.2",
    "uglify-js": "^3.17.4"
  }
}
